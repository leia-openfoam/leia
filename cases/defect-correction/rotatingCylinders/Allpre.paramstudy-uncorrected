#!/bin/sh
cd "${0%/*}" || exit                                # Run from this directory
. ${WM_PROJECT_DIR:?}/bin/tools/RunFunctions        # Tutorial run functions
#------------------------------------------------------------------------------

python ../create-study.py -c rotatingCylinders \
	-p rotatingCylinders-uncorrected.parameter \
	-s convergence-study-uncorrected 2>&1 | tee log.create-study-uncorrected

pyFoamRunParameterVariation.py --list-variations rotatingCylinders \
	rotatingCylinders-uncorrected.parameter 2>&1 | tee rotatningCylinders-uncorrected.variations

#------------------------------------------------------------------------------
