FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     leiaLevelSetFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         6.283185307179586;

CFL             0.3;

deltaT          1e-6;

writeControl    runTime;

writeInterval   0.3;

purgeWrite      0;

writeFormat     ascii;

writePrecision  16;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

#include "fobjects";
