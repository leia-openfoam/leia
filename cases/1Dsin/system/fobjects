// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
functions
{
    writeIsoSurfaceTopo
    {
        type            writeIsoSurfaceTopo;
        libs            ( fieldFunctionObjects leiaFunctionObjects );
        enabled         false;
    }
    gradPsiError
    {
        type            gradPsiError;
        libs            ( fieldFunctionObjects leiaFunctionObjects );
        field           psi;
        result          gradPsiError;
        writeControl    writeTime;
    }
    gradPsi
    {
        type            grad;
        libs            ( fieldFunctionObjects );
        field           psi;
        result          gradPsi;
        writeControl    timeStep;
        //writeControl    writeTime;
    }
    magGradPsi
    {
        type            mag;
        libs            ( fieldFunctionObjects );
        field           gradPsi;
        result          magGradPsi;
        writeControl    timeStep;
        //writeControl    writeTime;
    }
    gradPsiErrorCSV
    {
        type            gradPsiErrorCSV;
        libs            ( leiaFunctionObjects );
        writeControl    timeStep;
        field           gradPsiError;
    }
}


